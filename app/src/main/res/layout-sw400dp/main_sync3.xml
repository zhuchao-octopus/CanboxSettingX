<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="390px"
        android:layout_marginTop="0px"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="200px"
            android:layout_height="match_parent"
            android:layout_marginTop="0px"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/pp"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/pp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/prev"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/prev3" />

                <ImageView
                    android:id="@+id/next"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/next3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/repeat"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/common_repeat_tag" />

                <ImageView
                    android:id="@+id/random"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/common_shuffle_tag" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/speech"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/speech"
                    android:textSize="21px" />

                <Button
                    android:id="@+id/menu"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/menu"
                    android:textSize="21px" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/media"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/media"
                    android:textSize="21px" />

                <Button
                    android:id="@+id/info"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/info"
                    android:textSize="21px" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:gravity="center"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/app"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/app"
                    android:textSize="21px" />

                <Button
                    android:id="@+id/phone"
                    style="@style/ButonStyleNumSync3"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/phone"
                    android:textSize="21px" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="624px"
            android:layout_height="match_parent"
            android:layout_marginTop="0px"
            android:orientation="vertical" >

            <include layout="@layout/aux_player" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="0px"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/keyboard1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:orientation="vertical" >

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10px"
                    android:gravity="center" >

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num1"
                            style="@style/ButonStyleNumSync3"
                            android:background="@drawable/sub_background2"
                            android:text="@string/digit_1" />

                        <TextView
                            android:id="@+id/num2"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_2" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num3"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_3" />

                        <TextView
                            android:id="@+id/num4"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_4" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num5"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_5" />

                        <TextView
                            android:id="@+id/num6"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_6" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num7"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_7" />

                        <TextView
                            android:id="@+id/num8"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_8" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num9"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_9" />

                        <TextView
                            android:id="@+id/num_x"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_x0" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                    android:layout_marginTop="5px"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/num0"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_0" />

                        <TextView
                            android:id="@+id/num_j"
                            style="@style/ButonStyleNumSync3"
                            android:text="@string/digit_x1" />
                    </TableRow>
                </TableLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="68px"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/dial"
                        style="@style/ButonStyleNumSync3"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:scaleType="center"
                        android:src="@drawable/call" />

                    <ImageView
                        android:id="@+id/hang"
                        style="@style/ButonStyleNumSync3"
                        android:clickable="true"
                        android:onClick="onClick"
                        android:scaleType="center"
                        android:src="@drawable/hung" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="0px"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/keyboard2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="0px"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/up"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/u" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/left"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/l" />

                <TextView
                    android:id="@+id/ok"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:text="@android:string/ok" />

                <ImageView
                    android:id="@+id/right"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/r" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50px"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/down"
                    style="@style/ButonStyleNumSync3"
                    android:background="@drawable/sub_background2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/d" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60px"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="180px"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:background="#88000000"
            android:gravity="center"
            android:orientation="vertical" >

            <View
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="@drawable/common_list_line" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="@drawable/common_list_line" />

            <ImageView
                android:id="@+id/mic"
                style="@style/FocuseButtonStyle1"
                android:clickable="true"
                android:onClick="onClick"
                android:scaleType="center"
                android:src="@drawable/mic" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="@drawable/common_list_line" />

            <ImageView
                android:id="@+id/music"
                style="@style/FocuseButtonStyle1"
                android:clickable="true"
                android:onClick="onClick"
                android:scaleType="center"
                android:src="@drawable/music" />

            <View
                android:layout_width="match_parent"
                android:layout_height="2px"
                android:background="@drawable/common_list_line" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50px"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/sync"
                    android:layout_width="55px"
                    android:layout_height="wrap_content"
                    android:text="@string/sync"
                    android:textColor="#ffffff"
                    android:textSize="20px"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/info_icon"
                    android:layout_width="45px"
                    android:layout_height="wrap_content"
                    android:text="@string/info"
                    android:textColor="#ffffff"
                    android:textSize="20px"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/msg"
                    android:layout_width="45px"
                    android:layout_height="35px"
                    android:src="@drawable/msg"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/mic_icon"
                    android:layout_width="45px"
                    android:layout_height="30px"
                    android:scaleType="centerInside"
                    android:src="@drawable/mic"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/bt"
                    android:layout_width="45px"
                    android:layout_height="wrap_content"
                    android:src="@drawable/bt_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/phone_icon"
                    android:layout_width="45px"
                    android:layout_height="wrap_content"
                    android:src="@drawable/phone_icon"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/battery"
                    android:layout_width="45px"
                    android:layout_height="wrap_content"
                    android:src="@drawable/b0"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/signal"
                    android:layout_width="45px"
                    android:layout_height="wrap_content"
                    android:src="@drawable/s0"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="382px"
                android:layout_marginLeft="6px"
                android:background="@drawable/sub_background2"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingBottom="2px"
                    android:paddingLeft="2px"
                    android:paddingRight="2px"
                    android:paddingTop="2px" >

                    <LinearLayout
                        android:layout_width="540px"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:orientation="vertical" >

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />

                        <TextView
                            android:id="@+id/list1"
                            style="@style/listStyle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />

                        <TextView
                            android:id="@+id/list2"
                            style="@style/listStyle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />

                        <TextView
                            android:id="@+id/list3"
                            style="@style/listStyle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />

                        <TextView
                            android:id="@+id/list4"
                            style="@style/listStyle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />

                        <TextView
                            android:id="@+id/list5"
                            style="@style/listStyle" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="2px"
                            android:background="@drawable/common_list_line" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="108px"
                android:layout_alignParentBottom="true"
                android:layout_gravity="bottom"
                android:background="@drawable/bottom_background"
                android:gravity="right"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/button1"
                    style="@style/ButtonStyle2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/empty" />

                <Button
                    android:id="@+id/button2"
                    style="@style/ButtonStyle2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/empty" />

                <Button
                    android:id="@+id/button3"
                    style="@style/ButtonStyle2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/empty" />

                <Button
                    android:id="@+id/button4"
                    style="@style/ButtonStyle2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:text="@string/empty" />

                <ImageView
                    android:id="@+id/keyboard"
                    style="@style/ButtonStyle2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/keyboard" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/mask"
        android:layout_width="0px"
        android:layout_height="match_parent"
        android:background="#88000000"
        android:clickable="true" />

</LinearLayout>
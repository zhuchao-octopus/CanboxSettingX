<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_gravity="center|top"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/air_title_sync"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_dual" />

        <ImageView
            android:id="@+id/air_title_ce_rear"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_rear" />

        <ImageView
            android:id="@+id/air_title_ce_ac_1"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_ac_1" />

        <ImageView
            android:id="@+id/air_title_ce_inner_loop"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_inner_loop" />

        <ImageView
            android:id="@+id/air_title_ce_auto_large"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_auto_mid" />

        <ImageView
            android:id="@+id/icon_power"
            style="@style/AirButtonStyle1"
            android:src="@drawable/icon_power" />

        <!-- <ImageView -->
        <!-- android:id="@+id/air_title_ce_ac_max" -->
        <!-- style="@style/AirButtonStyle1" -->
        <!-- android:src="@drawable/air_title_ce_ac_max" /> -->
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginEnd="1.0dip"
        android:layout_marginStart="1.0dip"
        android:layout_marginTop="1.0dip"
        android:layout_weight="2"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/con_left_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_left_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_left_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="4.0"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/wind_up1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="onClick"
                        android:src="@drawable/air_content_max"
                        android:tag="ak_paramter=100,50" >
                    </ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="match_parent"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/wind_up2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:onClick="onClick"
                        android:src="@drawable/air_content_max"
                        android:tag="ak_paramter=100,50" >
                    </ImageView>
                </LinearLayout>
            </LinearLayout>

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="4.0" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/wind_down1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="35px"
                        android:src="@drawable/air_content_arrow_bottom" >
                    </ImageView>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="vertical" >

                        <FrameLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center" >

                            <ImageView
                                android:id="@+id/air_perple"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:onClick="onClick"
                                android:src="@drawable/air_content_air_people"
                                android:tag="ak_paramter=100,50" >
                            </ImageView>

                            <ImageView
                                android:id="@+id/wind_horizontal2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:src="@drawable/air_content_arrow_left" >
                            </ImageView>

                            <ImageView
                                android:id="@+id/wind_horizontal1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/air_content_arrow_right" >
                            </ImageView>
                        </FrameLayout>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/wind_down2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="35px"
                        android:src="@drawable/air_content_arrow_bottom" >
                    </ImageView>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_marginStart="1.0dip"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/con_right_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_right_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_right_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/wind_minus"
            style="@style/AirButtonStyle1"
        android:layout_weight="1.5"
            android:src="@drawable/wind_minus" />

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_marginTop="1.0dip"
            android:layout_weight="4"
            android:gravity="center"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="0px"
                android:layout_height="100px"
                android:layout_weight="4"
                android:gravity="bottom"
                android:orientation="horizontal"
                android:paddingLeft="10px"
                android:paddingRight="10px" >

                <ImageView
                    android:id="@+id/point0"
                    android:layout_width="0px"
                    android:layout_height="35px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point1"
                    android:layout_width="0px"
                    android:layout_height="43px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point2"
                    android:layout_width="0px"
                    android:layout_height="51px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point3"
                    android:layout_width="0px"
                    android:layout_height="59px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point4"
                    android:layout_width="0px"
                    android:layout_height="67px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point5"
                    android:layout_width="0px"
                    android:layout_height="75px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point6"
                    android:layout_width="0px"
                    android:layout_height="83px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible" >
                </ImageView>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/wind_add"
        android:layout_weight="1.5"
            style="@style/AirButtonStyle1"
            android:src="@drawable/wind_add" />
    </LinearLayout>

</LinearLayout>
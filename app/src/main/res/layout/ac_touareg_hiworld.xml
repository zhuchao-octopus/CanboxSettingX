<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_gravity="center|top"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/icon_power"
            style="@style/AirButtonStyle1"
            android:src="@drawable/icon_power" />

        <TextView
            android:id="@+id/air_title_ce_aqs"
            style="@style/AirButtonStyle1"
            android:text="@string/aqs"
            android:textColor="#ffffff"
            android:textSize="26px" />

        <ImageView
            android:id="@+id/air_title_ce_auto_large"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_auto_mid" />

        <ImageView
            android:id="@+id/air_title_ce_max"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_fwindow_heat" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginEnd="1.0dip"
        android:layout_marginStart="1.0dip"
        android:layout_marginTop="1.0dip"
        android:layout_weight="2"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/con_left_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_left_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_left_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="4.0"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.0dip"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/canbus21_mode1"
                    style="@style/AirButtonStyle1"
                    android:src="@drawable/canbus21_mode1" />

                <ImageView
                    android:id="@+id/canbus21_mode2"
                    style="@style/AirButtonStyle1"
                    android:src="@drawable/canbus21_mode3" />

                <ImageView
                    android:id="@+id/canbus21_mode5"
                    style="@style/AirButtonStyle1"
                    android:src="@drawable/canbus21_mode5" />

                <ImageView
                    android:id="@+id/wheel"
                    style="@style/AirButtonStyle1"
                    android:src="@drawable/wheel3" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_marginStart="1.0dip"
            android:layout_weight="1.5"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/con_right_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_right_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_right_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_marginTop="1.0dip"
            android:layout_weight="4.2"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/wind_minus"
                style="@style/AirButtonStyle1"
                android:src="@drawable/wind_minus" />

            <LinearLayout
                android:layout_width="0px"
                android:layout_height="100px"
                android:layout_weight="4"
                android:gravity="bottom"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/point0"
                    android:layout_width="0px"
                    android:layout_height="35px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point1"
                    android:layout_width="0px"
                    android:layout_height="43px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point2"
                    android:layout_width="0px"
                    android:layout_height="51px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point3"
                    android:layout_width="0px"
                    android:layout_height="59px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point4"
                    android:layout_width="0px"
                    android:layout_height="67px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point5"
                    android:layout_width="0px"
                    android:layout_height="75px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>

                <ImageView
                    android:id="@+id/point6"
                    android:layout_width="0px"
                    android:layout_height="83px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air4"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="visible"></ImageView>
            </LinearLayout>

            <ImageView
                android:id="@+id/wind_add"
                style="@style/AirButtonStyle1"
                android:src="@drawable/wind_add" />


        </LinearLayout>
    </LinearLayout>

</LinearLayout>
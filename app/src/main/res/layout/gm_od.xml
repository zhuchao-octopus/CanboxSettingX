<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_gravity="center|top"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/air_title_ce_max"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_max" />

        <ImageView
            android:id="@+id/air_title_ce_rear"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_rear" />

        <ImageView
            android:id="@+id/air_title_ce_ac_1"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_ac_1" />

        <ImageView
            android:id="@+id/air_title_ce_inner_loop"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_inner_loop" />

        <ImageView
            android:id="@+id/air_title_ce_auto_large"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_ce_auto_mid" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginEnd="1.0dip"
        android:layout_marginStart="1.0dip"
        android:layout_marginTop="1.0dip"
        android:layout_weight="2"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/con_left_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_left_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_left_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/con_mode_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <ImageView
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:clickable="false"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:src="@drawable/canbus21_mode2" />

            <ImageButton
                android:id="@+id/con_mode_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.0dip"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:orientation="horizontal" >

                <ImageButton
                    android:id="@+id/con_seathotright"
                    style="@style/AirButtonStyle1"
                    android:src="@drawable/img_air_seathotright0" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.0dip"
                android:layout_gravity="center"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:orientation="horizontal" >

                <ImageButton
                    android:id="@+id/con_seathotleft"
                    style="@style/AirButtonStyle1"
                    android:background="@drawable/button_air3"
                    android:src="@drawable/img_air_seathotleft0" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0.0dip"
            android:layout_height="match_parent"
            android:layout_marginStart="1.0dip"
            android:layout_weight="1.5"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/con_right_temp_up"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_up" />

            <TextView
                android:id="@+id/con_txt_right_temp"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/img_air_bg_sel"
                android:ellipsize="none"
                android:gravity="center"
                android:singleLine="true"
                android:textColor="@android:color/white"
                android:textSize="29.0sp" />

            <ImageButton
                android:id="@+id/con_right_temp_down"
                style="@style/AirButtonStyle1"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_marginTop="1.0dip"
                android:layout_weight="1.0"
                android:background="@drawable/button_air3"
                android:src="@drawable/img_air_temp_down" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_marginTop="1.0dip"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/air_title_sync"
            style="@style/AirButtonStyle1"
            android:src="@drawable/air_title_sync" />

        <ImageView
            android:id="@+id/wind_minus"
            style="@style/AirButtonStyle1"
            android:src="@drawable/wind_minus" />

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_marginTop="1.0dip"
            android:layout_weight="3"
            android:gravity="center"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="342px"
                android:layout_height="100px"
                android:gravity="bottom"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/point0"
                    android:layout_width="0px"
                    android:layout_height="35px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point1"
                    android:layout_width="0px"
                    android:layout_height="45px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point2"
                    android:layout_width="0px"
                    android:layout_height="55px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point3"
                    android:layout_width="0px"
                    android:layout_height="65px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point4"
                    android:layout_width="0px"
                    android:layout_height="75px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point5"
                    android:layout_width="0px"
                    android:layout_height="85px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>

                <ImageView
                    android:id="@+id/point6"
                    android:layout_width="0px"
                    android:layout_height="95px"
                    android:layout_weight="1"
                    android:background="@drawable/button_air2"
                    android:clickable="true"
                    android:onClick="onClick"
                    android:scaleType="center"
                    android:src="@drawable/point"
                    android:visibility="invisible" >
                </ImageView>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/wind_add"
            style="@style/AirButtonStyle1"
            android:src="@drawable/wind_add" />

        <ImageView
            android:id="@+id/icon_power"
            style="@style/AirButtonStyle1"
            android:src="@drawable/icon_power" />
    </LinearLayout>

</LinearLayout>